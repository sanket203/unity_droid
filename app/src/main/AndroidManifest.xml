<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unity.com.unityapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".unity.com.unityapp.base.view.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".unity.com.unityapp.base.view.HomeActivity" />

        <activity android:name=".unity.com.unityapp.base.view.RecentProfileDetailsActivity" />

        <activity android:name=".unity.com.unityapp.base.view.LoginActivity" />

        <activity android:name=".unity.com.unityapp.base.view.RegistrationActivity" />

        <activity android:name=".unity.com.unityapp.base.view.EditPersonalDetailsActivity" />

    </application>


</manifest>
